{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "JwtTokensConfiguration": {
      "ExpirationInMinutes": 7,
      "RefreshTokenExpirationInDays": 60
    }
  },
  "Whitelisting": {
    "UseWhitelisting": true
  }
}
